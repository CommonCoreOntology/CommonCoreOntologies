                                       cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CommonUserProfileOntology"^^xsd:anyURI ;
                                       rdfs:label "Telecommunication Network Identifier" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/TelecommunicationsNetworkAccount
cco:TelecommunicationsNetworkAccount rdf:type owl:Class ;
                                     rdfs:subClassOf cco:Contract ;
                                     cco:definition "A Contract between an Agent and a Telecommunications Service Provider consisting, at least in part, of a set of permissions recorded in a telecommunications system that govern the Agent's ability to interact with a determined portion of that system." ;
                                     cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CommonUserProfileOntology"^^xsd:anyURI ;
                                     rdfs:label "Telecommunications Network Account" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/TelephoneAccount
cco:TelephoneAccount rdf:type owl:Class ;
                     rdfs:subClassOf cco:TelecommunicationsNetworkAccount ;
                     cco:definition "A Telecommunications Network Account between an Agent and a Telephone Service Provider, which prescribes the assignment of one or more Telephone Number and Telecommunication Endpoint to allow the Agent to utilize telephony and data communications services." ;
                     cco:is_curated_in_ontology "http://www.ontologyrepository.com/CommonCoreOntologies/Domain/CommonUserProfileOntology"^^xsd:anyURI ;
                     rdfs:label "Telephone Account" .


###  http://www.ontologyrepository.com/CommonCoreOntologies/TelephoneNumber
cco:TelephoneNumber rdf:type owl:Class ;
